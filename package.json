{
  "name": "artemis",
  "version": "0.0.5",
  "description": "Artemis TS",
  "main": "index.js",
  "bin": {
    "arts": "./bin/artemis"
  },
  "config": {
    "build": [
      "mkdir -p build/web/src"
    ],
    "build_example": [
      "cp -f web/main.js build/web/main.js",
      "cp -f web/index.html build/web/index.html",
      "cp -f web/project_build.json build/web/project.json",
      "mkdir -p build/web/src",
      "cp -fr web/src/example build/web/src",
      "mkdir -p build/web/frameworks",
      "cp -fr web/frameworks/cocos2d-html5 build/web/frameworks",
      "cp -fr web/res build/web"
    ]
  },
  "scripts": {},
  "author": "darkoverlordofdata@gmail.com",
  "license": "MIT",
  "directories": {
    "lib": "./lib",
    "example": "./example"
  },
  "devDependencies": {
    "chai": "~1.9.2",
    "coffee-script": "1.9.0",
    "coffee2closure": "^0.1.12",
    "gulp": "^3.9.0",
    "gulp-gh-pages": "^0.5.2",
    "gulp-manifest": "0.0.7",
    "gulp-shell": "^0.4.2",
    "gulp-webserver": "^0.9.1",
    "liquid.coffee": "^0.1.1",
    "mocha": "~2.0.1",
    "morgan": "^1.6.1",
    "walker": "^1.0.7"
  },
  "dependencies": {
    "liquid.coffee": "^0.1.1",
    "mkdirp": "^0.5.1"
  }
}
